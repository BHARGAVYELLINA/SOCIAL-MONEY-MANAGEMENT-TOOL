    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;WELCOME TO SOCIAL MONEY MANAGEMENT TOOL&quot;" newline="True"/>
            <declare name="username, userpassword, usermail" type="String" array="True" size="11"/>
            <declare name="number" type="Integer" array="False" size=""/>
            <output expression="&quot;PLEASE ENTER THE NUMBER OF MEMBERS IN THE GROUP&quot; &amp; &quot;    &#13;&#10;MAXIMUM NUMBER OF USERS IN A GROUP IS 10 AND MINIMUM NUMBER OF USERS IN A GROUP IS 2&quot; &amp; &quot; &#13;&#10;(EXCLUDING THE ADMIN)&quot;" newline="True"/>
            <input variable="number"/>
            <if expression="number&lt;2 || number&gt;10">
                <then>
                    <output expression="&quot;MINIMUM NUMBER OF USERS IN A GROUP IS 2 AND MAXIMUM NUMBER OF USERS IN A GROUP IS 10&quot;" newline="True"/>
                    <breakpoint/>
                </then>
                <else/>
            </if>
            <call expression="ADMIN(username,userpassword,usermail,number)"/>
            <declare name="usernameL, usermailL, userpasswordL, usernameS, usermailS, userpasswordS, username1, usermail1, userpassword1" type="String" array="False" size=""/>
            <assign variable="usernameL" expression="&quot;a&quot;"/>
            <assign variable="usermailL" expression="&quot;a&quot;"/>
            <assign variable="userpasswordL" expression="&quot;a&quot;"/>
            <assign variable="usernameS" expression="&quot;a&quot;"/>
            <assign variable="usermailS" expression="&quot;a&quot;"/>
            <assign variable="userpasswordS" expression="&quot;a&quot;"/>
            <assign variable="username1" expression="&quot;a&quot;"/>
            <assign variable="usermail1" expression="&quot;a&quot;"/>
            <assign variable="userpassword1" expression="&quot;a&quot;"/>
            <call expression="CHECK(usernameL,usermailL, userpasswordL,usernameS,usermailS, userpasswordS, username,usermail,userpassword,number,username1,usermail1,userpassword1)"/>
            <declare name="debtnumber" type="Integer" array="False" size=""/>
            <assign variable="debtnumber" expression="random(10000000)"/>
            <declare name="choice" type="String" array="False" size=""/>
            <declare name="billinfo" type="String" array="True" size="8"/>
            <declare name="debtinfo" type="String" array="True" size="12"/>
            <declare name="amount, term" type="Integer" array="False" size=""/>
            <declare name="interest, EMI, debtamount" type="Real" array="False" size=""/>
            <declare name="type, EMIneed" type="String" array="False" size=""/>
            <assign variable="type" expression="&quot;borrow or lend&quot;"/>
            <assign variable="EMIneed" expression="&quot;yes&quot;"/>
            <assign variable="term" expression="1"/>
            <assign variable="EMI" expression="1"/>
            <assign variable="debtamount" expression="1"/>
            <output expression="&quot;WELCOME TO THE DEBT SECTION&quot;" newline="True"/>
            <output expression="&quot;ENTER THE AMOUNT FOR THE DEBT&quot;" newline="True"/>
            <input variable="amount"/>
            <output expression="&quot;ENTER THE INTEREST FOR THE DEBT&quot;" newline="True"/>
            <input variable="interest"/>
            <call expression="DEBTS(amount,interest,type,username1,usermail1,userpassword1,debtnumber,EMIneed,term,EMI,debtamount,debtinfo)"/>
            <call expression="BILL(number,billinfo)"/>
            <declare name="y" type="Integer" array="False" size=""/>
            <assign variable="y" expression="PAYMENTPAGE(debtinfo, billinfo)"/>
            <declare name="x" type="Integer" array="False" size=""/>
            <declare name="status" type="String" array="False" size=""/>
            <assign variable="x" expression="100"/>
            <assign variable="status" expression="&quot;payment_status&quot;"/>
            <call expression="SMMT(y)"/>
            <call expression="RECURRING"/>
            <call expression="EXPENSETRACKER"/>
            <call expression="REMAINDER"/>
            <output expression="&quot;THANK YOU FOR USING OUR APPLICATION.&quot;" newline="True"/>
        </body>
    </function>
    <function name="ADMIN" type="None" variable="">
        <parameters>
            <parameter name="username" type="String" array="True"/>
            <parameter name="usermail" type="String" array="True"/>
            <parameter name="userpassword" type="String" array="True"/>
            <parameter name="number" type="Integer" array="False"/>
        </parameters>
        <body>
            <declare name="i, age" type="Integer" array="False" size=""/>
            <output expression="&quot;PLEASE ENTER YOUR AGE&quot;" newline="True"/>
            <input variable="age"/>
            <if expression="age&gt;=16">
                <then>
                    <output expression="&quot;ENTER NAME OF THE ADMIN&quot;" newline="True"/>
                    <input variable="username[0]"/>
                    <output expression="&quot;PLEASE ENTER THE EMAIL ID OF THE ADMIN&quot;" newline="True"/>
                    <input variable="usermail[0]"/>
                    <output expression="&quot;PLEASE ENTER THE PASSWORD TO BE SET FOR THE ADMIN&quot;" newline="True"/>
                    <input variable="userpassword[0]"/>
                    <for variable="i" start="1" end="number" direction="inc" step="1">
                        <output expression="&quot;PLEASE ENTER YOUR AGE&quot;" newline="True"/>
                        <input variable="age"/>
                        <if expression="age&gt;=16">
                            <then>
                                <output expression="&quot;PLEASE ENTER THE NAME OF GROUP MEMBER &quot; &amp; ToString(i)" newline="True"/>
                                <input variable="username[i]"/>
                                <output expression="&quot;PLEASE ENTER THE EMAIL ID OF GROUP MEMBER &quot; &amp; ToString(i)" newline="True"/>
                                <input variable="usermail[i]"/>
                                <output expression="&quot;PLEASE ENTER THE PASSWORD TO BE SET FOR GROUP MEMBER &quot; &amp; ToString(i)" newline="True"/>
                                <input variable="userpassword[i]"/>
                            </then>
                            <else>
                                <output expression="&quot;AGE ELIGIBILITY IS NOT SATISFIED&quot;" newline="True"/>
                                <breakpoint/>
                            </else>
                        </if>
                    </for>
                    <output expression="&quot;USERGROUP1 IS CREATED&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;AGE ELIGIBILITY IS NOT SATISFIED&quot;" newline="True"/>
                    <breakpoint/>
                </else>
            </if>
        </body>
    </function>
    <function name="BILL" type="None" variable="">
        <parameters>
            <parameter name="members" type="Integer" array="False"/>
            <parameter name="billinfo" type="String" array="True"/>
        </parameters>
        <body>
            <output expression="&quot;WELCOME TO THE BILL SECTION&quot;" newline="True"/>
            <declare name="billid" type="Integer" array="False" size=""/>
            <declare name="absentmemberscount, i" type="Integer" array="False" size=""/>
            <declare name="billamount, amtduebyeachmember" type="Real" array="False" size=""/>
            <declare name="purpose, date" type="String" array="False" size=""/>
            <output expression="&quot;IS THERE ANY MEMBER WHO HAS NOT PARTICIPATED IN THIS ACTIVITY?&#13;&#10;IF YES THEN PLEASE ENTER THE NUMBER BELOW.&#13;&#10;IF NO THEN ENTER 0.&quot;" newline="True"/>
            <input variable="absentmemberscount"/>
            <if expression="absentmemberscount!=0">
                <then>
                    <declare name="absentmembersname" type="String" array="True" size="absentmemberscount"/>
                    <for variable="i" start="0" end="absentmemberscount-1" direction="inc" step="1">
                        <input variable="absentmembersname[i]"/>
                    </for>
                </then>
                <else/>
            </if>
            <assign variable="billid" expression="random(10000000)"/>
            <assign variable="billinfo[0]" expression="&quot;BILL ID=&quot;"/>
            <assign variable="billinfo[1]" expression="ToString(billid)"/>
            <output expression="&quot;PLEASE ENTER THE TOTAL BILL AMOUNT&quot;" newline="True"/>
            <input variable="billamount"/>
            <output expression="&quot;PLEASE ENTER THE PURPOSE OF THE BILL&quot;" newline="True"/>
            <input variable="purpose"/>
            <output expression="&quot;PLEASE ENTER THE DATE OF PAYMENT&#13;&#10;**PLEASE ENTER IN DOUBLE QUOTES&quot;" newline="True"/>
            <input variable="date"/>
            <assign variable="billinfo[2]" expression="&quot;Activity Name=&quot;"/>
            <assign variable="billinfo[3]" expression="purpose"/>
            <assign variable="amtduebyeachmember" expression="(billamount)/(members+1)"/>
            <assign variable="billinfo[4]" expression="&quot;Amount Due By Each Member=&quot;"/>
            <assign variable="billinfo[5]" expression="ToString(amtduebyeachmember)"/>
            <assign variable="billinfo[6]" expression="&quot;Date Of Activity=&quot;"/>
            <assign variable="billinfo[7]" expression="date"/>
            <assign variable="i" expression="0"/>
            <for variable="i" start="0" end="7" direction="inc" step="2">
                <output expression="billinfo[i] &amp; &quot; = &quot; &amp; billinfo[i+1]" newline="True"/>
            </for>
        </body>
    </function>
    <function name="CHECK" type="None" variable="">
        <parameters>
            <parameter name="usernameL" type="String" array="False"/>
            <parameter name="userpasswordL" type="String" array="False"/>
            <parameter name="usermailL" type="String" array="False"/>
            <parameter name="usernameS" type="String" array="False"/>
            <parameter name="usermailS" type="String" array="False"/>
            <parameter name="userpasswordS" type="String" array="False"/>
            <parameter name="username" type="String" array="True"/>
            <parameter name="usermail" type="String" array="True"/>
            <parameter name="userpassword" type="String" array="True"/>
            <parameter name="number" type="Integer" array="False"/>
            <parameter name="username1" type="String" array="False"/>
            <parameter name="usermail1" type="String" array="False"/>
            <parameter name="userpassword1" type="String" array="False"/>
        </parameters>
        <body>
            <declare name="credentials" type="String" array="False" size=""/>
            <declare name="i, otp, otp1, k, j, age" type="Integer" array="False" size=""/>
            <assign variable="otp1" expression="0"/>
            <output expression="&quot;ENTER LOGIN IF ACCOUNT EXISTS OR ENTER SIGNUP TO CREATE ACCOUNT&quot;" newline="True"/>
            <input variable="credentials"/>
            <if expression="credentials==&quot;login&quot;">
                <then>
                    <output expression="&quot;PLEASE ENTER YOUR USERNAME&quot;" newline="True"/>
                    <input variable="usernameL"/>
                    <output expression="&quot;PLEASE ENTER YOUR USERMAIL&quot;" newline="True"/>
                    <input variable="userpasswordL"/>
                    <output expression="&quot;PLEASE ENTER YOUR USERPASSWORD&quot;" newline="True"/>
                    <input variable="usermailL"/>
                    <assign variable="k" expression="0"/>
                    <assign variable="i" expression="0"/>
                    <while expression="i&lt;number+1 &amp;&amp; k==0">
                        <if expression="usernameL==username[i]">
                            <then>
                                <assign variable="k" expression="k+1"/>
                                <assign variable="j" expression="i"/>
                            </then>
                            <else/>
                        </if>
                        <assign variable="i" expression="i+1"/>
                    </while>
                    <assign variable="i" expression="0"/>
                    <while expression="i&lt;number+1 &amp;&amp; k==1">
                        <if expression="usermailL==usermail[i] &amp;&amp; i==j">
                            <then>
                                <assign variable="k" expression="k+1"/>
                            </then>
                            <else/>
                        </if>
                        <assign variable="i" expression="i+1"/>
                    </while>
                    <assign variable="i" expression="0"/>
                    <while expression="i&lt;number+1 &amp;&amp; k==2">
                        <if expression="userpasswordL==userpassword[i] &amp;&amp; i==j">
                            <then>
                                <assign variable="k" expression="k+1"/>
                            </then>
                            <else/>
                        </if>
                        <assign variable="i" expression="i+1"/>
                    </while>
                    <if expression="k==3">
                        <then>
                            <assign variable="otp" expression="random(10000)"/>
                            <output expression="otp" newline="True"/>
                            <output expression="&quot;PLEASE ENTER THE OTP YOU RECEIVED&quot;" newline="True"/>
                            <input variable="otp1"/>
                            <if expression="otp1==otp">
                                <then>
                                    <output expression="&quot;ACCOUNT SUCCESSFULLY LOGGED-IN&quot;" newline="True"/>
                                    <output expression="&quot;CONNECTED TO USERGROUP1&quot;" newline="True"/>
                                    <assign variable="username1" expression="username[j]"/>
                                    <assign variable="usermail1" expression="usermail[j]"/>
                                    <assign variable="userpassword1" expression="userpassword[j]"/>
                                </then>
                                <else>
                                    <output expression="&quot;OTP ENTERED IS WRONG&quot;" newline="True"/>
                                    <breakpoint/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;WRONG CREDENTIALS ENTERED, RE-CHECK YOUR CREDENTIALS&quot;" newline="True"/>
                            <breakpoint/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;PLEASE ENTER YOUR AGE&quot;" newline="True"/>
                    <input variable="age"/>
                    <if expression="age&gt;=16">
                        <then>
                            <output expression="&quot;PLEASE ENTER YOUR USERNAME&quot;" newline="True"/>
                            <input variable="usernameS"/>
                            <output expression="&quot;PLEASE ENTER YOUR USERMAIL&quot;" newline="True"/>
                            <input variable="usermailS"/>
                            <output expression="&quot;PLEASE ENTER YOUR USERPASSWORD&quot;" newline="True"/>
                            <input variable="userpasswordS"/>
                            <assign variable="otp" expression="random(10000)"/>
                            <output expression="otp" newline="True"/>
                            <output expression="&quot;PLEASE ENTER THE OTP YOU RECEIVED&quot;" newline="True"/>
                            <input variable="otp1"/>
                            <if expression="otp1==otp">
                                <then>
                                    <output expression="&quot;ACCOUNT SUCCESSFULLY REGISTERED&quot;" newline="True"/>
                                    <output expression="&quot;CONNECTED TO USERGROUP1&quot;" newline="True"/>
                                    <assign variable="i" expression="number+1"/>
                                    <assign variable="username[i]" expression="usernameS"/>
                                    <assign variable="usermail[i]" expression="usermailS"/>
                                    <assign variable="userpassword[i]" expression="userpasswordS"/>
                                    <assign variable="username1" expression="usernameS"/>
                                    <assign variable="usermail1" expression="usermailS"/>
                                    <assign variable="userpassword1" expression="userpasswordS"/>
                                </then>
                                <else>
                                    <output expression="&quot;OTP ENTERED IS WRONG&quot;" newline="True"/>
                                    <breakpoint/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;AGE ELIGIBILITY IS NOT SATISFIED&quot;" newline="True"/>
                            <breakpoint/>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
    <function name="DEBTS" type="None" variable="">
        <parameters>
            <parameter name="debtamount" type="Real" array="False"/>
            <parameter name="interest" type="Real" array="False"/>
            <parameter name="type" type="String" array="False"/>
            <parameter name="username1" type="String" array="False"/>
            <parameter name="usermail1" type="String" array="False"/>
            <parameter name="userpassword1" type="String" array="False"/>
            <parameter name="debtnumber" type="Integer" array="False"/>
            <parameter name="EMIneed" type="String" array="False"/>
            <parameter name="term" type="Integer" array="False"/>
            <parameter name="EMI" type="Real" array="False"/>
            <parameter name="amount" type="Integer" array="False"/>
            <parameter name="debtinfo" type="String" array="True"/>
        </parameters>
        <body>
            <output expression="&quot;ENTER BORROW TO TAKE MONEY OR ENTER LEND TO GIVE MONEY&quot;" newline="True"/>
            <input variable="type"/>
            <if expression="type==&quot;borrow&quot;">
                <then>
                    <output expression="&quot;IF YOU ARE OPTING FOR EMI PLEASE ENTER YES ELSE NO&quot;" newline="True"/>
                    <input variable="EMIneed"/>
                    <if expression="EMIneed==&quot;yes&quot;">
                        <then>
                            <declare name="debtid, i" type="Integer" array="False" size=""/>
                            <declare name="lender, borrower, date" type="String" array="False" size=""/>
                            <assign variable="debtid" expression="random(10000000)"/>
                            <assign variable="debtinfo[0]" expression="&quot;DEBT ID=&quot;"/>
                            <assign variable="debtinfo[1]" expression="ToString(debtid)"/>
                            <output expression="&quot;PLEASE ENTER THE NAME OF THE BORROWER&quot;" newline="True"/>
                            <input variable="borrower"/>
                            <output expression="&quot;PLEASE ENTER THE NAME OF THE LENDER&quot;" newline="True"/>
                            <input variable="lender"/>
                            <output expression="&quot;PLEASE ENTER THE DATE OF PAYMENT&#13;&#10;**PLEASE ENTER IN DOUBLE QUOTES&quot;" newline="True"/>
                            <input variable="date"/>
                            <assign variable="debtinfo[2]" expression="&quot;Borrower Name:&quot;"/>
                            <assign variable="debtinfo[3]" expression="ToString(borrower)"/>
                            <assign variable="debtinfo[4]" expression="&quot;Lender Name&quot;"/>
                            <assign variable="debtinfo[5]" expression="ToString(lender)"/>
                            <assign variable="debtinfo[6]" expression="&quot;Date Of Payment:&quot;"/>
                            <assign variable="debtinfo[7]" expression="date"/>
                            <assign variable="i" expression="0"/>
                            <output expression="&quot;ENTER NUMBER OF MONTHS&quot;" newline="True"/>
                            <input variable="term"/>
                            <assign variable="debtamount" expression="(amount*(interest/(12*100))*((1 + (interest/(12*100)))^term/(((1 + (interest/(12*100)))^term)-1)))*1000"/>
                            <assign variable="debtinfo[8]" expression="&quot;Debt Amount:&quot;"/>
                            <assign variable="debtinfo[9]" expression="ToString(debtamount)"/>
                            <assign variable="debtinfo[10]" expression="&quot;Activity:&quot;"/>
                            <assign variable="debtinfo[11]" expression="&quot;BORROWING WITH EMI&quot;"/>
                            <for variable="i" start="0" end="11" direction="inc" step="2">
                                <output expression="debtinfo[i] &amp; &quot; = &quot; &amp; debtinfo[i+1]" newline="True"/>
                            </for>
                        </then>
                        <else>
                            <declare name="debtid, i" type="Integer" array="False" size=""/>
