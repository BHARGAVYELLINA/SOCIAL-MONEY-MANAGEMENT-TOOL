    <function name="RECURRING" type="None" variable="">
        <parameters/>
        <body>
            <declare name="otp3, otp4, amount1, timefrequency, bankno, upipin1" type="Integer" array="False" size=""/>
            <assign variable="otp3" expression="random(10000)"/>
            <output expression="&quot;WELCOME TO RECURRING PAYMENT SECTION&quot;" newline="True"/>
            <declare name="purpose1, select, agreement, subscription" type="String" array="False" size=""/>
            <output expression="&quot;ENTER AGREED IF YOU AGREE WITH THE TERMS AND CONDITIONS&quot;" newline="True"/>
            <input variable="agreement"/>
            <if expression="agreement==&quot;agreed&quot;">
                <then>
                    <output expression="&quot;SETTING UP RECURRING PAYMENT&quot;" newline="True"/>
                    <output expression="&quot;PLEASE ENTER THE PURPOSE OF THE RECURRING PAYMENT&quot;" newline="True"/>
                    <input variable="purpose1"/>
                    <output expression="&quot;ENTER AMOUNT&quot;" newline="True"/>
                    <input variable="amount1"/>
                    <output expression="&quot;CHOOSE TIME FREQUENCY [NO. OF DAYS]&quot;" newline="True"/>
                    <input variable="timefrequency"/>
                    <output expression="&quot;ENTER BANK TO PAY THROUGH BANK GATEWAY&#13;&#10;ENTER UPI TO PAY THROUGH UPI GATEWAY&quot;" newline="True"/>
                    <input variable="select"/>
                    <if expression="select==&quot;bank&quot;">
                        <then>
                            <output expression="&quot;ENTER CARD NUMBER&quot;" newline="True"/>
                            <input variable="bankno"/>
                        </then>
                        <else>
                            <output expression="&quot;ENTER UPI PIN&quot;" newline="True"/>
                            <input variable="upipin1"/>
                        </else>
                    </if>
                    <output expression="otp3" newline="True"/>
                    <output expression="&quot;ENTER THE OTP RECEIVED&quot;" newline="True"/>
                    <input variable="otp4"/>
                    <if expression="otp3==otp4">
                        <then>
                            <output expression="&quot;AMOUNT PROCESSING&quot;" newline="True"/>
                            <output expression="&quot;PAYMENT SUCCESSFUL&#13;&#10;AMOUNT SENT TO MERCHANT&quot;" newline="True"/>
                            <output expression="&quot;RECURRING PAYMENT ON &quot; &amp; purpose1 &amp; &quot; OCCUR AFTER &quot; &amp; timefrequency &amp; &quot; DAYS&quot;" newline="True"/>
                            <output expression="&quot;ENTER SUBSCRIBE TO LET THE RECURRING PAYMENT OCCUR &#13;&#10;OR&#13;&#10;ENTER STOP TO END THE RECURRING PAYMENT&quot;" newline="True"/>
                            <input variable="subscription"/>
                            <if expression="subscription==&quot;subscribe&quot;">
                                <then>
                                    <output expression="&quot;SUBSCRIBED&quot;" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot;UNSUBSCRIBED&quot;" newline="True"/>
                                </else>
                            </if>
                        </then>
                        <else>
                            <output expression="&quot;OTP ENTERED IS WRONG&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;TERMS AND CONDITIONS ARE NOT AGREED&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
    <function name="REMAINDER" type="None" variable="">
        <parameters/>
        <body>
            <declare name="payment1, timefrequency1" type="String" array="False" size=""/>
            <output expression="&quot;ENTER PAID IF PAYMENT IS DONE OR ENTER NOT PAID IF PAYMENT IS NOT DONE&quot;" newline="True"/>
            <input variable="payment1"/>
            <if expression="payment1==&quot;paid&quot;">
                <then>
                    <output expression="&quot;REMAINDER IS OFF&quot;" newline="True"/>
                </then>
                <else>
                    <while expression="payment1!=&quot;paid&quot;">
                        <assign variable="timefrequency1" expression="&quot;24hrs&quot;"/>
                        <output expression="&quot;COMPLETE THE PAYMENT&quot;" newline="True"/>
                        <output expression="&quot;ENTER PAID IF PAYMENT IS DONE OR ENTER NOT PAID IF PAYMENT IS NOT DONE&quot;" newline="True"/>
                        <input variable="payment1"/>
                    </while>
                    <output expression="&quot;REMAINDER IS OFF&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
