                            <output expression="&quot;PLEASE ENTER THE NAME OF THE BORROWER&quot;" newline="True"/>
                            <input variable="borrower"/>
                            <output expression="&quot;PLEASE ENTER THE NAME OF THE LENDER&quot;" newline="True"/>
                            <input variable="lender"/>
                            <output expression="&quot;PLEASE ENTER THE DATE OF PAYMENT&#13;&#10;**PLEASE ENTER IN DOUBLE QUOTES&quot;" newline="True"/>
                            <input variable="date"/>
                            <assign variable="debtinfo[2]" expression="&quot;Borrower Name:&quot;"/>
                            <assign variable="debtinfo[3]" expression="ToString(borrower)"/>
                            <assign variable="debtinfo[4]" expression="&quot;Lender Name&quot;"/>
                            <assign variable="debtinfo[5]" expression="ToString(lender)"/>
                            <assign variable="debtinfo[6]" expression="&quot;Date Of Payment:&quot;"/>
                            <assign variable="debtinfo[7]" expression="date"/>
                            <assign variable="i" expression="0"/>
