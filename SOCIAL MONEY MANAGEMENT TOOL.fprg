                            <for variable="i" start="0" end="11" direction="inc" step="2">
                                <output expression="debtinfo[i] &amp; &quot; = &quot; &amp; debtinfo[i+1]" newline="True"/>
                            </for>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;IF BORROWER IS OPTING FOR EMI PLEASE ENTER YES ELSE NO&quot;" newline="True"/>
                    <input variable="EMIneed"/>
                    <if expression="EMIneed==&quot;yes&quot;">
